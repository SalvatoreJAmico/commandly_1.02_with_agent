# Copy this file to .env and fill in your actual values

# OpenAI API Key - Get this from https://platform.openai.com/api-keys
OPENAI_API_KEY=your_openai_api_key_here

# Optional: OpenAI API Base URL (uncomment if using a different endpoint)
# OPENAI_API_BASE=https://api.openai.com/v1

# Commandly permissions
COMMANDLY_ALLOW_WRITE=true
COMMANDLY_FULL_CONTROL=true